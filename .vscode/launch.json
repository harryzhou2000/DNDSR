    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Attach",
            "type": "cppdbg",
            "request": "attach",
            // "program": "${workspaceFolder}/build/test/meshSerial_Test.exe",
            "program": "${workspaceFolder}/build/test/vrStatic_Test.exe",
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            // "program": "${workspaceFolder}/build/test/arrayDerived_test.exe",
            "program": "${workspaceFolder}/build/test/serializerJSON_Test.exe",
            // "program": "${workspaceFolder}/build/test/vrBasic_Test.exe",
            // "program": "${workspaceFolder}/build/test/vrStatic_Test.exe",
            // "program": "${workspaceFolder}/build/test/meshSerial_Test.exe",
            // "program": "${workspaceFolder}/build/test/euler.exe",
            // "program": "${workspaceFolder}/build/test/euler3D.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/build",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}